{
  "type":"object",
  "$schema":"http://json-schema.org/draft-03/schema",
  "required":true,
  "properties":{
    "id":{
      "type":"string",
      "required":false,
      "description":"unique identifier for the entity"
    },
    "name":{
      "type":"string",
      "required":false,
      "description":"name of the entity"
    },
    "entries":{
      "type":"array",
      "required":false,
      "items":{
        "type":"object",
        "properties":{
          "value":{
            "type":"string",
            "required":false
          },
          "synonyms":{
            "type":"array",
            "required":false,
            "items":{
              "type":"string",
              "required":false
            }
          }
        }
      }
    },
    "isEnum":{
      "type":"boolean",
      "required":false
    },
    "automatedExpansion":{
      "type":"boolean",
      "required":false
    }
  }
}
